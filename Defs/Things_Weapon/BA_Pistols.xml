<Defs>
    <!--===== Pistol Category ====-->

    <!-- Jezail Short musket -->
    <ThingDef ParentName="BaseHumanMakeableGun">
        <defName>BA_Gun_Jezail_P</defName>
        <label>Jezail Short musket</label>
        <description>A traditional firearm with an iconic curved stock, widely used by tribes.</description>
        <graphicData>
            <texPath>Things/Weapon/Jezail_Pistol</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>1.2</drawSize>
        </graphicData>
        <techLevel>Neolithic</techLevel>
        <comps>
            <!-- <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon"> </li> -->
            <li Class="BlackArmory.RainFireOffShoot_CompProperties">
                <FireOffWeather>
                    <li>Fog</li>
                    <li>FoggyRain</li>
                    <li>Rain</li>
                    <li>RainyThunderstorm</li>
                    <li>SnowHard</li>
                    <li>SnowGentle</li>
                </FireOffWeather>
                <ThickCount>5</ThickCount>
                <!--<Smoke>Mote_Smoke</Smoke>-->
            </li>
        </comps>
        <soundInteract>Interact_Rifle</soundInteract>
        <statBases>
            <WorkToMake>12000</WorkToMake>
            <Mass>1.5</Mass>
            <AccuracyTouch>0.7</AccuracyTouch>
            <AccuracyShort>0.45</AccuracyShort>
            <AccuracyMedium>0.15</AccuracyMedium>
            <AccuracyLong>0.1</AccuracyLong>
            <RangedWeapon_Cooldown>3</RangedWeapon_Cooldown>
        </statBases>
        <equippedAngleOffset>0</equippedAngleOffset>
        <weaponTags>
            <li>NeolithicRangedHeavy</li>
            <li>NeolithicRangedChief</li>
        </weaponTags>
        <costList>
            <WoodLog>25</WoodLog>
            <Steel>12</Steel>
        </costList>
        <recipeMaker>
            <recipeUsers>
                <li>CraftingSpot</li>
                <li>BA_FueledGunSmithy</li>
            </recipeUsers>
            <researchPrerequisite>BA_Thunderer</researchPrerequisite>
            <skillRequirements>
                <Crafting>2</Crafting>
            </skillRequirements>
        </recipeMaker>
        <verbs>
            <li>
                <verbClass>BlackArmory.Verb_RainFireOffShoot</verbClass>
                <!--<varbClass>Verb_Shoot</varbClass>-->
                <hasStandardCommand>true</hasStandardCommand>
                <defaultProjectile>BA_Bullet_BasicRound</defaultProjectile>
                <warmupTime>1.2</warmupTime>
                <range>25.9</range>
                <soundCast>BA_Musket</soundCast>
                <soundCastTail>GunTail_Heavy</soundCastTail>
                <muzzleFlashScale>9</muzzleFlashScale>
            </li>
        </verbs>
        <tools>
            <li>
                <label>stock</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>6</power>
                <cooldownTime>1.2</cooldownTime>
            </li>
            <li>
                <label>barrel</label>
                <capacities>
                    <li>Blunt</li>
                    <li>Poke</li>
                </capacities>
                <power>6</power>
                <cooldownTime>1.2</cooldownTime>
            </li>
        </tools>
    </ThingDef>

